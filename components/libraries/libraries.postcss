.libraries-wr {
    display: flex;
    align-items: center;
    flex-direction: row;
    justify-content: center;
}

.grid-wr {
    padding : 8px;
    display: flex;
    align-items: center;
    justify-content: center;

    .grid-item {
        margin: 4px;
    }

    .red {
        color: red;
    }
    .green {
        color: #008000
    }

    .nothing-to-see {
        font-size: 2rem;
        margin-top: 25%;
    }
}